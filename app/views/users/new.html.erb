<h1>Sign up!</h1>
<%= form_for User.new do |f| %>
	<p>
		<%= f.label :username %>
		<%= f.text_field :username %>
	</p>
	<p>
		<%= f.label :email %>
		<%= f.email_field :email %>
	</p>
	<p>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</p>
	<p>
		<%= f.label :location %>
		<%= f.text_field :location %>
	</p>
	<p>
		<%= f.label :bio %><br>
		<%= f.text_area :bio %>
	</p>
	<p>
		<%= f.submit "Sign up!" %>
	</p>
<% end %>
